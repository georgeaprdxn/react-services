{
	"name": "reactjs-boilerplate",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"@types/anymatch": "^1.3.1",
		"@types/enzyme": "^3.10.5",
		"@types/enzyme-adapter-react-16": "^1.0.6",
		"@types/jest": "^24.0.0",
		"@types/node": "^12.0.0",
		"@types/react": "^16.9.0",
		"@types/react-dom": "^16.9.0",
		"@types/react-redux": "^7.1.7",
		"@types/react-router-dom": "^5.1.5",
		"@types/react-select": "^3.0.12",
		"@types/rx": "^4.1.1",
		"@types/styled-components": "^5.1.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-redux": "^7.2.0",
		"react-router-dom": "^5.1.2",
		"react-scripts": "3.4.1",
		"react-select": "^3.1.0",
		"redux": "^4.0.5",
		"redux-observable": "^1.2.0",
		"reset-css": "^5.0.1",
		"rxjs": "^6.5.5",
		"rxjs-compat": "^6.5.5",
		"styled-components": "^5.1.0",
		"typescript": "~3.7.2"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"format": "prettier --write '**/*.{ts,tsx}'",
		"styleguide": "styleguidist server --config config/styleguide.config.js --port 5000",
		"styleguide-build": "styleguidist build --config config/styleguide.config.js",
		"create-module": "node config/module.config.js"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@typescript-eslint/parser": "^2.29.0",
		"enzyme-to-json": "^3.4.4",
		"husky": "^4.2.5",
		"inquirer": "^7.1.0",
		"lint-staged": "^10.1.7",
		"prettier": "^2.0.5",
		"react-docgen-typescript": "^1.16.4",
		"react-styleguidist": "^11.0.5",
		"tslint": "^6.1.2",
		"tslint-config-prettier": "^1.18.0",
		"tslint-react": "^5.0.0"
	},
	"jest": {
		"snapshotSerializers": [
			"enzyme-to-json/serializer"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"tslint --fix",
			"prettier --write",
			"git add"
		]
	}
}
